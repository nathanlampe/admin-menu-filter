{
	"name": "nathanlampe/admin-menu-filter",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"description": "Advanced phonetic admin menu filter for WordPress",
	"homepage": "https://nathanlampe.com",
	"authors": [
		{
			"name": "Nathan Lampe",
			"email": "nlampe@gmail.com",
			"homepage": "https://www.nathanlampe.com"
		}
	],
	"keywords": [
		"wordpress", "wp", "admin menu filter", "menu filter", "menu sort", "menu search"
	],
	"support": {
		"issues": "https://nathanlampe.com",
		"forum": "https://nathanlampe.com/contact"
	},
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true,
		"allow-plugins": {
			"composer/installers": true
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"php": ">=7.1",
		"composer/installers": "^1.0 | ^2.0"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "*",
		"wp-coding-standards/wpcs": "0.10.0"
	},
	"autoload": {
		"psr-4": {
			"AdminMenuFilter\\": "inc"
		}
	},
	"scripts": {
		"phpcs": "phpcs --standard=phpcs.xml"
	}
}
